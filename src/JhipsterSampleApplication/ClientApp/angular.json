{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jhipster-sample-application": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "jhi",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": {
              "base": "dist",
              "browser": ""
            },            
            "index": "src/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/content",
              "src/favicon.ico",
              "src/manifest.webapp",
              "src/robots.txt",
              {
                "glob": "**/*",
                "input": "src/swagger-ui",
                "output": "/swagger-ui/"
              },
              {
                "glob": "swagger-ui.css",
                "input": "node_modules/swagger-ui-dist",
                "output": "/swagger-ui/"
              },
              {
                "glob": "swagger-ui-bundle.js",
                "input": "node_modules/swagger-ui-dist",
                "output": "/swagger-ui/"
              },
              {
                "glob": "swagger-ui-standalone-preset.js",
                "input": "node_modules/swagger-ui-dist",
                "output": "/swagger-ui/"
              },
              {
                "glob": "favicon-16x16.png",
                "input": "node_modules/swagger-ui-dist",
                "output": "/swagger-ui/"
              },
              {
                "glob": "favicon-32x32.png",
                "input": "node_modules/swagger-ui-dist",
                "output": "/swagger-ui/"
              },
              {
                "glob": "axios.min.js",
                "input": "node_modules/axios/dist",
                "output": "/swagger-ui/"
              }
            ],
            "styles": [
              "src/content/scss/vendor.scss",
              "src/content/scss/global.scss"
            ],
            "scripts": [],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "250kb",
                  "maximumError": "500kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "buildTarget": "jhipster-sample-application:build:development",
            "host": "127.0.0.1",
            "port": 4200,
            "proxyConfig": "proxy.conf.json",
            "allowedHosts": [
              "creative-systems-inc.com",
              "www.creative-systems-inc.com"
            ]
          },
          "configurations": {
            "production": {
              "buildTarget": "jhipster-sample-application:build:production"
            },
            "development": {
              "buildTarget": "jhipster-sample-application:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "configPath": "jest.conf.js",
            "tsConfig": "tsconfig.spec.json"
          }
        }
      }
    },
    "ngx-query-builder": {
      "projectType": "library",
      "root": "projects/ngx-query-builder",
      "sourceRoot": "projects/ngx-query-builder/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ngx-query-builder/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-query-builder/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-query-builder/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ngx-query-builder/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "popup-ngx-query-builder": {
      "projectType": "library",
      "root": "projects/popup-ngx-query-builder",
      "sourceRoot": "projects/popup-ngx-query-builder/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/popup-ngx-query-builder/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/popup-ngx-query-builder/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/popup-ngx-query-builder/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/popup-ngx-query-builder/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "popup-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/popup-demo",
      "sourceRoot": "projects/popup-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/popup-demo",
            "index": "projects/popup-demo/src/index.html",
            "browser": "projects/popup-demo/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/popup-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/popup-demo/public"
              }
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "projects/popup-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "3MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "8kB",
                  "maximumError": "12kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "popup-demo:build:production"
            },
            "development": {
              "buildTarget": "popup-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/popup-demo/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/popup-demo/public"
              }
            ],
            "styles": [
              "node_modules/primeicons/primeicons.css",
              "projects/popup-demo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "cache": {
      "enabled": true,
      "path": "./../../../tmp/angular/",
      "environment": "all"
    },
    "packageManager": "npm",
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}