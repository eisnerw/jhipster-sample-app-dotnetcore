{
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning"
    },
    "SyslogEnabled": false,
    "SyslogUrl": "localhost",
    "SyslogPort": "6514",
    "SyslogAppName": "JhipsterSampleApplication",
    "Enrich": ["FromLogContext", "WithProcessId", "WithThreadId"],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u4} {ProcessId} --- [{ThreadId,3}] {SourceContext} : {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
    ,
    "Elasticsearch": {
      "IndexFormat": "app-logs"
    }
  },
  "AllowedHosts": "*",
  "EntityFramework": {
    "DataSource": ":memory:"
  },
  "Security": {
    "Authentication": {
